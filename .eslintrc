{
    "parser": "babel-eslint",
    "env": {
      "es6": true,
      "browser": true,
      "node": true,
      "jest/globals": true
    },
    "rules": {
      "semi": "off",
      "arrow-parens": "off",
      "comma-dangle": "off",
      "linebreak-style": ["error", "windows"],
      "indent": ["error", 2, { "SwitchCase": 1 }],
      "require-jsdoc": "off",
      "operator-linebreak": "off",
      "no-debugger": "off"
    },
    "plugins": ["jest"],
    "extends": ["eslint:recommended", "google"]
  }
  